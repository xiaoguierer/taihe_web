# 开发环境配置
NODE_ENV=development
VITE_APP_TITLE=我的Vue应用（开发环境）
VITE_APP_VERSION=1.0.0
VITE_APP_BUILD_TIME=2024-01-01

# API 配置
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# 功能开关
VITE_FEATURE_AUTH_ENABLED=true
VITE_FEATURE_ANALYTICS_ENABLED=false
VITE_FEATURE_DEBUG_ENABLED=true

# 第三方服务配置
VITE_APP_GOOGLE_ANALYTICS_ID=
VITE_APP_SENTRY_DSN=

# 开发工具配置
VITE_APP_VUE_DEVTOOLS_ENABLED=true
VITE_APP_REDUX_DEVTOOLS_ENABLED=true

# 模拟数据配置
VITE_APP_MOCK_API_ENABLED=true
VITE_APP_MOCK_DELAY=500

# 路由配置
VITE_APP_ROUTER_MODE=history
VITE_APP_ROUTER_BASE=/

# 主题配置
VITE_APP_THEME=light
VITE_APP_PRIMARY_COLOR=#409EFF
VITE_APP_SECONDARY_COLOR=#67C23A

# 分页配置
VITE_APP_PAGE_SIZE=20
VITE_APP_PAGE_SIZES=[10, 20, 50, 100]

# 上传配置
VITE_APP_UPLOAD_MAX_SIZE=10485760
VITE_APP_UPLOAD_ACCEPT=image/*,.pdf,.doc,.docx

# 地图配置（如果需要）
VITE_APP_MAP_API_KEY=
VITE_APP_MAP_CENTER=[116.397428, 39.90923]

# WebSocket 配置
VITE_APP_WS_URL=ws://localhost:3000/ws
VITE_APP_WS_RECONNECT_INTERVAL=3000
