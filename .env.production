# 生产环境配置
NODE_ENV=production
VITE_APP_TITLE=我的Vue应用
VITE_APP_VERSION=1.0.0
VITE_APP_BUILD_TIME=2024-01-01

# API 配置
VITE_API_BASE_URL=https://api.example.com
VITE_API_TIMEOUT=15000

# 功能开关
VITE_FEATURE_AUTH_ENABLED=true
VITE_FEATURE_ANALYTICS_ENABLED=true
VITE_FEATURE_DEBUG_ENABLED=false

# 第三方服务配置
VITE_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_APP_SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx@sentry.io/xxxxxx

# 开发工具配置
VITE_APP_VUE_DEVTOOLS_ENABLED=false
VITE_APP_REDUX_DEVTOOLS_ENABLED=false

# 模拟数据配置
VITE_APP_MOCK_API_ENABLED=false
VITE_APP_MOCK_DELAY=0

# 路由配置
VITE_APP_ROUTER_MODE=history
VITE_APP_ROUTER_BASE=/

# 主题配置
VITE_APP_THEME=light
VITE_APP_PRIMARY_COLOR=#409EFF
VITE_APP_SECONDARY_COLOR=#67C23A

# 分页配置
VITE_APP_PAGE_SIZE=20
VITE_APP_PAGE_SIZES=[10, 20, 50, 100]

# 上传配置
VITE_APP_UPLOAD_MAX_SIZE=10485760
VITE_APP_UPLOAD_ACCEPT=image/*,.pdf,.doc,.docx

# 地图配置（如果需要）
VITE_APP_MAP_API_KEY=your_production_map_api_key
VITE_APP_MAP_CENTER=[116.397428, 39.90923]

# WebSocket 配置
VITE_APP_WS_URL=wss://api.example.com/ws
VITE_APP_WS_RECONNECT_INTERVAL=5000

# CDN 配置
VITE_APP_CDN_URL=https://cdn.example.com
VITE_APP_STATIC_RESOURCE_URL=https://static.example.com

# 性能监控配置
VITE_APP_PERFORMANCE_MONITOR_ENABLED=true
VITE_APP_ERROR_REPORT_ENABLED=true

# 安全配置
VITE_APP_CSP_ENABLED=true
VITE_APP_HTTPS_ENFORCED=true

# 缓存配置
VITE_APP_CACHE_VERSION=v1.0.0
VITE_APP_CACHE_MAX_AGE=31536000

# PWA 配置（如果使用PWA）
VITE_APP_PWA_ENABLED=true
VITE_APP_PWA_NAME=我的Vue应用
VITE_APP_PWA_THEME_COLOR=#409EFF
VITE_APP_PWA_BACKGROUND_COLOR=#ffffff
